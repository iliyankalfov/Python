
public class Area {
	private int length;
	private int breadth;
	
	public void setDim(int len, int bre) {
		this.length = len;
		this.breadth = bre;
	}
	
	public int getArea() {
		return this.length * this.breadth;
	}

	public static void main(String[] args) {
		Area area = new Area();
		area.setDim(5, 10);
		System.out.println(area.getArea());

	}

}
